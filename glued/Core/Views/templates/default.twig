<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
  <title>{{ pageTitle }}</title>
  <!-- CSS Files -->
  {{ assets({files: [
    '@public/assets/node_modules/bootstrap/dist/css/bootstrap.min.css',
    '@public/assets/node_modules/stisla/assets/css/style.css',
    '@public/assets/node_modules/stisla/assets/css/components.css',
    '@public/assets/css/glued.css',
    ], name: 'styles'}) }}
  <link rel="stylesheet" href="/assets/node_modules/@fortawesome/fontawesome-free/css/all.min.css">
</head>

<body>
  <div id="app">
    <div class="main-wrapper main-wrapper-1">
      <div class="navbar-bg"></div>
      <nav class="navbar navbar-expand-lg main-navbar">
        <form class="form-inline mr-auto">
          <ul class="navbar-nav mr-3">
            <li><a href="#" data-toggle="sidebar" class="nav-link nav-link-lg"><i class="fas fa-bars"></i></a></li>
            <li><a href="#" data-toggle="search" class="nav-link nav-link-lg d-sm-none"><i class="fas fa-search"></i></a></li>
          </ul>
          <div class="search-element">
            <input class="form-control" type="search" placeholder="Search" aria-label="Search" data-width="250">
            <button class="btn" type="submit"><i class="fas fa-search"></i></button>
            <div class="search-backdrop"></div>
            <div class="search-result">
              <div class="search-header">
                Notice
              </div>
              <div class="search-item">
                <a href="#">Search is currently disabled</a>
                <a href="#" class="search-close"><i class="fas fa-times"></i></a>
              </div>
            </div>
          </div>
        </form>
         {% include '/Core/Views/templates/partials/menu-anon.twig' %}
      </nav>
      <div class="main-sidebar sidebar-style-2">
        <aside id="sidebar-wrapper">
          <div class="sidebar-brand">
            <a href="/">Glued</a>
          </div>
          <div class="sidebar-brand sidebar-brand-sm">
            <a href="/">Gl</a>
          </div>
          <ul class="sidebar-menu">
            <li class="menu-header">Docs &amp; tutorials</li>
            <li class="active">
              <a href="#" class="nav-link has-dropdown"><i class="fas fa-fire"></i><span>Tutorial</span></a>
              <ul class="dropdown-menu">
                <li><a class="nav-link" href="/tutorial/">Tutorial home</a></li>
                <li><a class="nav-link" href="/tutorial/01">Controllers</a></li>
              </ul>
            </li>
            <li class="menu-header">Account</li>
            <li class="dropdown">
              <a href="#" class="nav-link has-dropdown" data-toggle="dropdown"><i class="fas fa-columns"></i> <span>My settings</span></a>
              <ul class="dropdown-menu">
                <li><a class="nav-link" href="#">Profile</a></li>
                <li><a class="nav-link" href="#">Account</a></li>
              </ul>
            </li>
            <li class="menu-header">{{ Apps }}</li>
            <li class="dropdown">
              <a href="#" class="nav-link has-dropdown" data-toggle="dropdown"><i class="fas fa-columns"></i> <span>Membership</span></a>
              <ul class="dropdown-menu">
                <li><a class="nav-link" href="#">Status</a></li>
                <li><a class="nav-link" href="#">Ballance</a></li>
              </ul>
            </li>
            <li class="dropdown">
              <a href="#" class="nav-link has-dropdown" data-toggle="dropdown"><i class="fas fa-columns"></i> <span>Worklog</span></a>
              <ul class="dropdown-menu">
                <li><a class="nav-link" href="#">My log</a></li>
                <li><a class="nav-link" href="#">Statistics</a></li>
              </ul>
            </li>

        </aside>
      </div>

      <!-- Main Content -->
      <div class="main-content">
      {% block content %}{% endblock %}  
      </div>
      <footer class="main-footer">
        <div class="footer-left">
          Copyright &copy; 2019 <div class="bullet"></div> Made By <a href="https://industra.space/">INDUSTRA</a>
        </div>
        <div class="footer-right">
          
        </div>
      </footer>
    </div>
  </div>

  {{ assets({files: [
    '@public/assets/node_modules/jquery/dist/jquery.min.js',
    '@public/assets/node_modules/popper.js/dist/umd/popper.min.js',
    '@public/assets/node_modules/tooltip.js/dist/umd/tooltip.min.js',
    '@public/assets/node_modules/bootstrap/dist/js/bootstrap.min.js',
    '@public/assets/node_modules/nicescroll/dist/jquery.nicescroll.min.js',
    '@public/assets/node_modules/moment/min/moment.min.js',
    '@public/assets/node_modules/stisla/assets/js/stisla.js',
    ], name: 'stisla-general.js'}) }}


  {{ assets({files: [
    '@public/assets/node_modules/stisla/assets/js/scripts.js',
    '@public/assets/node_modules/stisla/assets/js/custom.js'
    ], name: 'stisla-template.js'}) }}

</body>
</html>
